DefaultDirName={pf}\{#MyAppName}
DisableProgramGroupPage=yes
LicenseFile=C:\Users\andri\IdeaProjects\DGen\LICENSE
OutputDir=C:\Users\andri\IdeaProjects\DGen\setup
