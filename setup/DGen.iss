Source: "C:\Users\andri\IdeaProjects\DGen\build\jfx\app\properties\*"; DestDir: "{app}\properties"; Flags: ignoreversion recursesubdirs
; NOTE: Don't use "Flags: ignoreversion" on any shared system files

[Icons]
